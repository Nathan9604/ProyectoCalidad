@{
    ViewData["Title"] = "Pasajeros";
}

<h1>Consulta sobre Pasajeros</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Consulta">
            <div class="form-group">
                <label class="control-label" for="FechaInicial">Fecha Inicial</label>
                <input class="form-control" type="date" data-val="true" data-val-required="El campo Fecha Inicial es requerido." id="FechaInicial" name="FechaInicial" value="">
                <label class="control-label" for="FechaFinal">Fecha Final</label>
                <input class="form-control" type="date" data-val="true" data-val-required="El campo Fecha Final es requerido." id="FechaFinal" name="FechaFinal" value="">
            </div>
            <div class="form-group">
                <input type="submit" value="Consultar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>